		<!-- InstanceBeginEditable name="Scripts" -->
		<!-- TemplateBeginIf cond="video==false" -->
		<script>
			$script.ready('all-scripts', function() {
				$(document).ready(function() {				
					$("a[rel='enclosure']").colorbox({
						photo:true, 
						transition: 'fade'
					});		
				});
			});
		</script>
		<!-- TemplateEndIf -->
		<!-- TemplateBeginIf cond="video" -->
		<script>
			$script.ready('all-scripts', function() {				
				//stackoverflow.com/questions/886864/differences-between-using-wmodetransparent-opaque-window-for-an-embedded 
				//= "window" fixes severe choppiness in IE 7-8. Do not set to opaque, nor anything else.
				//However, this fix breaks Colorbox only in Flash mode! and not in HTML5 video mode.
				// = "transparent fixes flash overlay, but makes video really choppy and almost unwatchable
						
				$(document).ready(function() {
					jwplayer("player").setup({	
						skin: "/assets/jwplayer/lulu.zip",
						stretching: 'uniform',
						smoothing: true,
						wmode: "window",
						modes: [
							{type: "html5"},
							{type: "flash", src: "/assets/flash/player.swf"},
							{type: "download", src: "@@(video_poster)@@"}
						],
						'controlbar.idlehide': true
					});
					
					$("a[rel='enclosure']").colorbox({
						photo:true, 
						transition: 'fade',
						onOpen:function() {
							//change wmode of jwplayer to opaque
							jwplayer("player").remove();
						},
						onClosed:function() {					
							//change wmode of jwplayer back to window
							jwplayer("player").setup({	
								skin: "/assets/jwplayer/lulu.zip",
								stretching: 'uniform',
								smoothing: true,
								wmode: "window",
								modes: [
									{type: "html5"},
									{type: "flash", src: "/assets/flash/player.swf",
										config: {
											//for IE7?
											type: 'video'
										}
									},
									{type: "download", src: "@@(video_poster)@@"}
								],
								'controlbar.idlehide': true
							});
						}
					});		
				});
			});
		</script> 
